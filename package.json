{
  "name": "notion2obsidian",
  "version": "2.6.0",
  "description": "High-performance CLI tool to migrate Notion exports to Obsidian-compatible markdown format",
  "type": "module",
  "main": "./notion2obsidian.js",
  "bin": {
    "notion2obsidian": "./notion2obsidian.js"
  },
  "exports": {
    ".": "./notion2obsidian.js",
    "./lib/*": "./src/lib/*.js"
  },
  "files": [
    "notion2obsidian.js",
    "src/lib/assets.js",
    "src/lib/callouts.js",
    "src/lib/cli.js",
    "src/lib/csv.js",
    "src/lib/enrich.js",
    "src/lib/frontmatter.js",
    "src/lib/links.js",
    "src/lib/scanner.js",
    "src/lib/stats.js",
    "src/lib/utils.js",
    "src/lib/zip.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "migrate": "bun run notion2obsidian.js",
    "dry-run": "bun run notion2obsidian.js --dry-run",
    "help": "bun run notion2obsidian.js --help",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitbonsai/notion2obsidian.git"
  },
  "bugs": {
    "url": "https://github.com/bitbonsai/notion2obsidian/issues"
  },
  "homepage": "https://bitbonsai.github.io/notion2obsidian/",
  "dependencies": {
    "chalk": "^5.3.0",
    "fflate": "^0.8.2",
    "gray-matter": "^4.0.3",
    "ora": "^9.0.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "notion",
    "obsidian",
    "migration",
    "markdown",
    "converter",
    "cli",
    "export",
    "import",
    "notes",
    "knowledge-base"
  ],
  "author": "bitbonsai",
  "license": "MIT"
}
